meme_dict = {
    "CRINGE": "Garip ya da utandırıcı bir şey",
    "LOL": "Komik bir şeye verilen cevap",
    "ROFL": "Bir şakaya karşılık cevap",
    "SHEESH": "Onaylamamak",
    "CREEPY": "Korkunç",
    "AGGRO": "Agresifleşmek/sinirlenmek"
}

while True:
    word = input("Anlamadığınız bir kelime yazın (hepsini büyük harflerle yazın veya çıkmak için Q yazın!): ")
    if word == "Q":
        print("Çıkış yapılıyor...")
        break
    elif word in meme_dict:
        print(meme_dict[word])
    else:
        print("Geçersiz kelime. Kelime listesi: LOL, CRINGE, ROFL, SHEESH, CREEPY, AGGRO")
